{
  "datasets": [
    {
      "name": "997ddea8",
      "displayName": "DS_PhonePe",
      "queryLines": [
        "SELECT\r\n",
        "    year_month,\r\n",
        "    SUM(total_amount) AS total_spend,\r\n",
        "    SUM(transaction_count) AS total_transactions\r\n",
        "FROM ppe.gold.gold_monthly_expenses\r\n",
        "GROUP BY year_month\r\n",
        "ORDER BY year_month DESC;\r\n"
      ]
    },
    {
      "name": "c7986a54",
      "displayName": "gold_daily_expenses",
      "queryLines": [
        "SELECT * FROM ppe.gold.gold_daily_expenses"
      ]
    },
    {
      "name": "16d6ed2d",
      "displayName": "gold_monthly_expenses",
      "queryLines": [
        "SELECT * FROM ppe.gold.gold_monthly_expenses"
      ]
    },
    {
      "name": "07c18a54",
      "displayName": "gold_merchant_summary",
      "queryLines": [
        "SELECT * FROM ppe.gold.gold_merchant_summary"
      ]
    },
    {
      "name": "a052ce50",
      "displayName": "Monthly KPI summary",
      "queryLines": [
        "SELECT\r\n",
        "    year_month,\r\n",
        "    SUM(total_amount) AS total_spend,\r\n",
        "    SUM(transaction_count) AS total_transactions\r\n",
        "FROM ppe.gold.gold_monthly_expenses\r\n",
        "GROUP BY year_month\r\n",
        "ORDER BY year_month DESC;\r\n"
      ]
    },
    {
      "name": "10e90ff8",
      "displayName": "Category-wise spending",
      "queryLines": [
        "SELECT\r\n",
        "    category,year_month,\r\n",
        "    SUM(total_amount) AS total_spend\r\n",
        "FROM ppe.gold.gold_monthly_expenses\r\n",
        "GROUP BY category,year_month\r\n",
        "ORDER BY total_spend DESC;\r\n"
      ]
    },
    {
      "name": "2755c312",
      "displayName": "Daily expense trend",
      "queryLines": [
        "SELECT\r\n",
        "    transaction_date,year_month,\r\n",
        "    SUM(total_amount) AS daily_spend\r\n",
        "FROM ppe.gold.gold_daily_expenses\r\n",
        "GROUP BY transaction_date,year_month\r\n",
        "ORDER BY transaction_date;\r\n"
      ]
    },
    {
      "name": "5ba408db",
      "displayName": "Top merchants",
      "queryLines": [
        "SELECT\r\n",
        "    merchant,year_month,\r\n",
        "    SUM(total_amount) AS total_spend,\r\n",
        "    SUM(transaction_count) AS txn_count\r\n",
        "FROM ppe.gold.gold_merchant_summary\r\n",
        "GROUP BY merchant,year_month\r\n",
        "ORDER BY total_spend DESC\r\n",
        "LIMIT 10;\r\n"
      ]
    },
    {
      "name": "f5e53579",
      "displayName": "Year Fiter",
      "queryLines": [
        "SELECT DISTINCT year_month FROM ppe.gold.gold_monthly_expenses ORDER BY year_month DESC;\r\n"
      ]
    },
    {
      "name": "3f0beb2e",
      "displayName": "investment_count",
      "queryLines": [
        "SELECT\r\n",
        "    SUM(total_amount) AS investment_count\r\n",
        "FROM ppe.gold.gold_monthly_expenses\r\n",
        "WHERE category = 'Shares_Investments';\r\n"
      ]
    },
    {
      "name": "f00d254f",
      "displayName": "total_debit_amount",
      "queryLines": [
        "SELECT\r\n",
        "    SUM(debit_amount) AS total_debit_amount\r\n",
        "FROM ppe.gold.gold_daily_expenses;\r\n"
      ]
    },
    {
      "name": "4063f63e",
      "displayName": "total_credit_amount",
      "queryLines": [
        "SELECT\r\n",
        "    SUM(credit_amount) AS total_credit_amount\r\n",
        "FROM ppe.gold.gold_daily_expenses;\r\n"
      ]
    }
  ],
